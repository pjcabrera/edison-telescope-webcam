{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "semistandard",
    "standard-react"
  ],
  "env": {
    "commonjs": true,
    "es6": true
  },
  "plugins": [
    "babel"
  ],
  "rules": {
    "space-before-function-paren": [2, "never"],
    "jsx-quotes": [2, "prefer-double"],
    "react/jsx-curly-spacing": [2, "never"],
    "no-class-assign": 0
  }
}
